#!/bin/bash
 
src=$1
dst=$2

cp $src/error.xml $dst
cp $src/extra_score.txt $dst
cp $src/extra.marker $dst
cp $src/extra.swc $dst
cp $src/extra.txt $dst
cp $src/graph_d.swc $dst
cp $src/miss_score.txt $dst
cp $src/miss.marker $dst
cp $src/miss.swc $dst
cp $src/miss.txt $dst
cp $src/nearby.marker $dst
cp $src/nearby.txt $dst
cp $src/tube.swc $dst