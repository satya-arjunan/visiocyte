i=66
while [ $i -le 152 ]
do
  if [ $i -lt 10 ]; then
    id=00"$i"
  elif [ $i -lt 100 ]; then
    id=0"$i"
  else
    id=$i
  fi
  echo $id
  bin/diadem_d1_split /Volumes/myerslab/zhaot/DIADEM_NM/label/"$id".tif -o /Volumes/myerslab/zhaot/DIADEM_NM/split/"$id".tif
  i=`expr $i + 1`
done

